<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<name>XMLDataStorePlugin</name>
	<class>net.scientifichooliganism.xmldatastore.XMLDataStorePlugin</class>
	<config>
		<module>XMLDataStorePlugin</module>
		<sequence>0</sequence>
		<key>active</key>
		<value>true</value>
	</config>
	<config>
		<module>XMLDataStorePlugin</module>
		<sequence>1</sequence>
		<key>storage</key>
		<value>true</value>
	</config>
	<config>
		<module>XMLDataStorePlugin</module>
		<sequence>2</sequence>
		<key>depends</key>
		<value>XMLPlugin</value>
	</config>
	<config>
		<module>XMLDataStorePlugin</module>
		<sequence>3</sequence>
		<key>default_file</key>
		<value>XMLPersist.xml</value>
	</config>
	<action>
		<name>addResource</name>
		<description>add a resource to the collection of resources used by this plugin</description>
		<module>XMLDataStorePlugin</module>
		<class>net.scientifichooliganism.xmldatastore.XMLDataStorePlugin</class>
		<!--url></url-->
		<method>addResource</method>
	</action>
	<action>
		<name>query</name>
		<description>query data from an XML backed data store</description>
		<module>XMLDataStorePlugin</module>
		<class>net.scientifichooliganism.xmldatastore.XMLDataStorePlugin</class>
		<!--url></url-->
		<method>query</method>
	</action>
	<action>
		<name>persist</name>
		<description>Persists a ValueObject to persistent storage.</description>
		<module>XMLDataStorePlugin</module>
		<class>net.scientifichooliganism.xmldatastore.XMLDataStorePlugin</class>
		<method>persist</method>
	</action>
</plugin>